	<queryString language="SQL">
		<![CDATA[SELECT register_number,
	end_payment_date,
	customer_code,
	customer_name_candidate,
	activity_code,
	start_activity_date,
	tel_no,
	fax_no,
	email,
	example_name,
	branch_name,
	activity_name,
	price,
	company_th_receipt,
	add_receipt,
	province_name,
	amphur_name,
	tambon_name,
	decode( ,,,) tambon,
	decode( ,,,) amphur,
	decode( ,,,) province,
	postcode_receipt postcode
FROM olp_register r
	inner join olp_applicant ap ON 
	 'NULL' = 'NULL' 
	inner join olp_register_activity ra ON 
	 'NULL' = 'NULL' 
	inner join olp_activity a ON 
	 'NULL' = 'NULL' 
	inner join olp_example ex ON 
	 'NULL' = 'NULL' 
	inner join olp_plan pln ON 
	 'NULL' = 'NULL' 
	inner join olp_company c ON 
	 'NULL' = 'NULL' 
	inner join glb_province p ON 
	 'NULL' = 'NULL' 
	inner join glb_district d ON 
	 'NULL' = 'NULL' 
	left outer join glb_tambon t ON 
	 'NULL' = 'NULL' 
WHERE 
	 'NULL' = 2,555 
	 AND 'NULL' = 2,860 
ORDER BY id ASC,
	register_number ASC,
	id ASC,
	id ASC,
	id ASC]]>
	</queryString>
